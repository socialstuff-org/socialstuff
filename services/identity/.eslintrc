{
  "extends": "../../.eslintrc",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "commonjs": true
  },
  "plugins": [
    "@typescript-eslint",
    "eslint-plugin-node"
  ]
}
